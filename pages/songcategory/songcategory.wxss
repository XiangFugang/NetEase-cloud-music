.tltie_shell {
  padding: 80rpx 0 0 30rpx;
  display: flex;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 10;
}

.tltie_img {
  width: 50rpx;
  height: 50rpx;
  filter: brightness(100%);
}

.list {
  padding-left: 30rpx;
}

.text {
  color: black;
}

.tltie_text {
  padding-top: 8rpx;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 20rpx;
  color: #e6e6e6;
  letter-spacing: 2rpx;
}

.Rotationchart{
  padding-top: 150rpx;
}
.imges {
  width: 95%;
  margin-left: 2.5%;
  height: 300rpx;
  border-radius: 15rpx;
}




.music_shell {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.music_dv {
  padding-bottom: 40rpx;
}

.music_img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 12rpx;
}

.music_text {
  font-size: 27rpx;
  width: 200rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.Load{
position: relative;
}
.Loading{
 position: absolute;
 top: 0;
 left: 50%;
 margin-left: -100rpx;
}