.music_shell {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.music_dv {
  padding-bottom: 40rpx;
}

.music_img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 12rpx;
}

.music_text {
  font-size: 27rpx;
  width: 200rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.Load{
position: relative;
}
.Loading{
 position: absolute;
 top: 0;
 left: 50%;
 margin-left: -100rpx;
}